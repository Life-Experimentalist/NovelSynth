{
  "manifestVersion": 3,
  "name": "novelsynth",
  "version": "2.0.0",
  "description": "AI-powered content enhancement for web reading",
  "main": "index.js",
  "author": "",
  "license": "MIT",
  "homepage": "https://life-experimentalist.github.io/novelsynth",
  "repository": {
    "type": "git",
    "url": "https://github.com/LifeExperimentalist/novelsynth.git"
  },
  "bugs": {
    "url": "https://github.com/LifeExperimentalist/novelsynth/issues"
  },
  "keywords": [
    "chrome-extension",
    "ai",
    "content-enhancement",
    "reading"
  ],
  "scripts": {
    "start": "webpack --watch --progress --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "dev": "webpack --watch --progress --config webpack.config.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "extensionInfo": {
    "supportedBrowsers": [
      "chrome",
      "firefox",
      "edge"
    ],
    "defaultProviders": 5,
    "defaultHandlers": 10
